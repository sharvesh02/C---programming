#include <stdio.h>

int main() {
    int date,month,year;
    printf("Enter a date: ");
    scanf("%d/%d/%d",&date,&month,&year);

    if (year % 400 == 0 || (year % 100 != 0 && year % 4 == 0)) {
        printf("%d is a leap year.\n", year);
    } else {
        printf("%d is not a leap year.\n", year);
    }
    return 0;
}
